<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
  "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module rename-to='mygridapp'>
  <inherits name='com.google.gwt.user.User'/>
  <entry-point class='org.vaadin.artur.mygridapp.client.MyGridApp'/>
  <source path='client'/>

  <!--  Use Vaadin widgets -->
  <inherits name='com.vaadin.Vaadin'/>
  
  <!-- Inherit to use the whole default Valo theme -->
<!--   <inherits name='com.vaadin.themes.Valo'/> -->
  <!-- Inherit this instead to use a customized Valo for the whole page-->
<!--   <inherits name='org.vaadin.artur.mytheme.MyValo'/> -->
  <!-- Inherit this instead to use a customized Valo for only the grid -->
<!--   <inherits name='org.vaadin.artur.mytheme.MyValoPrefixed'/> -->
  <!-- Inherit this instead to use a customized Valo for only the grid -->
  <inherits name='org.vaadin.artur.mytheme.MyGridOnlyTheme'/>

  <collapse-all-properties/>
</module>
